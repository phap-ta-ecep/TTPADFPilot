				}
			}
		],
		"parameters": {
			"listConfigId_S": {
				"type": "string",
				"defaultValue": "1,2,3,4,5,6,7,8,9"
			},
			"listConfigId_Ar": {
				"type": "array",
				"defaultValue": [
					"1",
					"2",
					"3",
					"4",
					"5",
					"6",
					"7",
					"8",
					"9"
